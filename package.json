{
  "name": "enterprise-antd",
  "version": "0.0.2",
  "description": "",
  "keywords": [],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "author": "vfa25",
  "repository": {
    "type": "git",
    "url": "https://github.com/vfa25/ts_antd_components.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "pub": "sh publish.sh",
    "lint:js": "eslint --fix --ext .jsx --ext .js ./examples",
    "prebuild": "rimraf dist",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "eslint": "eslint --ext .tsx,.ts,.js,.jsx --fix ./components",
    "precommit": "npm run eslint && lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "clean": "bubai run clean",
    "dist": "bubai run dist",
    "compile": "bubai run compile",
    "dev": "cross-env npx webpack-dev-server --mode=development --config examples/webpack.config.js"
  },
  "lint-staged": {
    "{components}/**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^2.1.1",
    "@ant-design/icons-react": "^2.0.1",
    "antd": "^3.21.4",
    "babel-runtime": "6.x",
    "bubai": "git+https://github.com/vfa25/react-tools-webpack.git",
    "classnames": "^2.2.6",
    "create-react-context": "^0.3.0",
    "enterprise-antd": "^0.0.1",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-lifecycles-compat": "^3.0.4",
    "webpack": "^4.28.4",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-conventional": "^7.1.2",
    "@hot-loader/react-dom": "^16.8.6",
    "@packtracker/webpack-plugin": "^2.1.0",
    "@types/classnames": "^2.2.8",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.14.10",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "@types/react-lifecycles-compat": "^3.0.1",
    "@types/webpack": "^4.4.27",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@webpack-cli/init": "^0.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.2",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "body-parser": "^1.18.3",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-jsx-control-statements": "^2.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-tinker": "^0.5.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jest-config": "^23.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "memory-fs": "^0.4.1",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "react-hot-loader": "^4.8.2",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.16",
    "shelljs": "^0.8.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^23.10.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.3",
    "url-loader": "^1.1.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
